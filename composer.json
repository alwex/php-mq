{
    "name": "alwex/php-q",
    "description": "plain old php message queue system with a PDO driven database backend",
    "homepage": "https://github.com/alwex/php-q",
    "type": "library",
    "license": "GNU",
    "authors": [
        {
            "name": "Alexandre GUIDET",
            "email": "guidet.alexandre@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "bin": [
        "bin/phpq"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": ">=5.3.0",
        "symfony/console": "~2",
        "symfony/process": "~2.8",
        "symfony/config": "~2.8",
        "cocur/slugify": "~1.0",
        "symfony/yaml": "~2.8",
        "doctrine/orm": "2.4.*",
        "monolog/monolog": "^1.20",
        "react/react": "^0.4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {"PhpQ\\": "src/PhpQ"}
    },
    "autoload-dev": {
        "psr-4": { "PhpQ\\": "tests/PhpQ" }
    }
}
